#!/bin/bash

FASTCGI_USER=www-data
FASTCGI_GROUP=www-data
SOCKET=/var/run/php-fastcgi/php-fastcgi-5.3.socket
PIDFILE=/var/run/php-fastcgi/php-fastcgi-5.3.pid
CHILDREN=6
PHP5=/opt/phpfarm/inst/bin/php-cgi-5.3.23

/usr/bin/spawn-fcgi -s $SOCKET -P $PIDFILE -C $CHILDREN -u $FASTCGI_USER -g $FASTCGI_GROUP -f $PHP5